<?xml version="1.0" encoding="UTF-8"?>

<Primitive-Attributes>
    <Primitive-Attribute>
		  <name>String</name>
	  	  <database-datatype>varchar(1000)</database-datatype>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>Boolean</name>
	  	  <database-datatype>DECIMAL(1,0)</database-datatype>
    </Primitive-Attribute>
    
     <Primitive-Attribute>
		  <name>Integer</name>
	  	  <database-datatype>Decimal(31,0)</database-datatype>
	  	  <digits-before-decimal>31</digits-before-decimal>
	  	  <digits-after-decimal>0</digits-after-decimal>
    </Primitive-Attribute>
    
     <Primitive-Attribute>
		  <name>Long</name>
	  	  <database-datatype>DECIMAL(31)</database-datatype>
	  	  <digits-before-decimal>31</digits-before-decimal>
	  	  <digits-after-decimal>0</digits-after-decimal>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>Double</name>
	  	  <database-datatype>DOUBLE PRECISION</database-datatype>
	  	  <digits-before-decimal>23</digits-before-decimal>
	  	  <digits-after-decimal>15</digits-after-decimal>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>Float</name>
	  	  <database-datatype>Float(53)</database-datatype>
	  	  <digits-before-decimal>32</digits-before-decimal>
	  	  <digits-after-decimal>6</digits-after-decimal>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>Short</name>
	  	  <database-datatype>DECIMAL(19)</database-datatype>
	  	  <digits-before-decimal>18</digits-before-decimal>
	  	  <digits-after-decimal>0</digits-after-decimal>
    </Primitive-Attribute>
    
     <Primitive-Attribute>
		  <name>Date</name>
	  	  <database-datatype>Date</database-datatype>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>DateTime</name>
	  	  <database-datatype>TimeStamp</database-datatype>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>File</name>
	  	  <database-datatype>blob(1g)</database-datatype>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>Object</name>
	  	  <database-datatype>blob(1g)</database-datatype>
    </Primitive-Attribute>
    
</Primitive-Attributes>
